package aug_31;
import java.util.*;

public class codes {
	public static int search(int []arr,int i, int []copy,int n) {
		for(int j=0;i<n;i++)
		{
			if(arr[i]==copy[j]) {
				return j;
			}
		}
		return -1;
	}

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int [] arr = new int[n];
		for(int i=0;i<n;i++)
			arr[i] = sc.nextInt();
		int [] copy = Arrays.copyOf(arr, n);
		Arrays.sort(copy);
		for(int i=0;i<n;i++)
		{
			int k =search(arr,i,copy,n);
			arr[i]=k;
		}
		System.out.println(Arrays.toString(arr));

	}

}

package arrays;
import java.util.*;

public class array{
	public static int fibo(int n)
	{
		int series=0;
		if(n==0 || n==1)               
			return n;                           
		int a=fibo(n-1);
		int b= fibo(n-2);
		series = a+b;
        System.out.print(series);
        return series;
	}
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		fibo(n);
	}	
}
package tests;
import java.util.*;

public class shams {
	public static void rec(String que,int ci,String ans)
	{
		if(ci==que.length()) {
			System.out.println(ans);
			return;}
		if(que.charAt(ci)=='A' && que.charAt(ci+1)=='B' || que.charAt(ci)=='B' && que.charAt(ci+1)=='A')
		{
			rec(que,ci+1,ans+(ci-1));
		}
		if(que.charAt(ci)=='C' && que.charAt(ci+1)=='D' || que.charAt(ci)=='D' && que.charAt(ci+1)=='C') {
			rec(que,ci+1,ans+(ci-1));
		}
		rec(que,ci+1,ans);
	}

	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);
		String que = sc.nextLine();
		int ci=0;
		rec(que,ci,"");
	}

}
